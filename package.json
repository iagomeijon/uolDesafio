{
  "name": "UOL",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rfv yarn.lock pakage.lock.json dist/ node_modules/",
    "build": "react-scripts build",
    "dev": "next --port 3000",
    "eject": "react-scripts eject",
    "format": "prettier --write **/*.{js,ts,tsx,scss,css,json}",
    "isready": "yarn format && yarn lint && yarn build",
    "lint": "eslint '**/*.{js,ts,tsx}' --color --fix",
    "reinstall": "yarn clean && yarn --force",
    "rebuild": "yarn clean && yarn --force && yarn start",
    "start": "react-scripts start",
    "test": "jest --env=jsdom",
    "test:watch": "yarn test -- --watch",
    "test:ci": "yarn test -- --coverage --runInBand"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.1",
    "babel": "^6.23.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-preset-react-app": "^10.0.0",
    "clsx": "^1.1.1",
    "env-cmd": "^10.1.0",
    "jest": "^27.0.6",
    "jest-css-modules": "^2.1.0",
    "js-cookie": "^2.2.1",
    "next": "^10.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.10",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.43",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
