tion getRepositoriesList(): Promise<void> {\r\n    setIsLoading(true);\r\n    if (user) {\r\n      await axios\r\n        .get(`https://api.github.com/users/${user.login}/repos`)\r\n        .then((res) => {\r\n          console.log(res.data);\r\n        })\r\n        .catch()\r\n        .finally(() => setIsLoading(false));\r\n    }\r\n  }\r\n\r\n  function clean(): void {\r\n    setuUserNotFound(false);\r\n    setIsLoading(false);\r\n    setRepositories(null);\r\n    setUser(null);\r\n  }\r\n\r\n  return {\r\n    state: {\r\n      isLoading,\r\n      user,\r\n      userNotFound,\r\n    },\r\n    actions: {\r\n      getUser,\r\n      getRepositoriesList,\r\n      clean,\r\n    },\r\n  };\r\n}\r\n"]},"metadata":{},"sourceType":"module"}